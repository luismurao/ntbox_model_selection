journal = {Theor. Ecol.},
keywords = {conceptual framework,law,model,theory},
number = {1},
pages = {21--28},
publisher = {Springer Netherlands},
title = {{A general theory of ecology}},
url = {http://www.springerlink.com/index/10.1007/s12080-007-0002-0},
volume = {1},
year = {2007}
}
