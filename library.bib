isbn = {0035-9203 (Print)$\backslash$r0035-9203 (Linking)},
issn = {00359203},
journal = {Trans. R. Soc. Trop. Med. Hyg.},
keywords = {Aedes aegypti,Dengue,Ecological niche modeling,Mexico,Remote sensing,Spatiotemporal variation},
