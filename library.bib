issn = {18703453},
journal = {Rev. Mex. Biodivers.},
keywords = {Ecological niche models,Natural habitat,Natural protected areas,Potential distribution,Threatened species,Transformed habitat},
number = {3},
pages = {926--950},
title = {{Distribuci{\'{o}}n y conservaci{\'{o}}n de especies amenazadas en Mesoam{\'{e}}rica, Choc{\'{o}} y Andes tropicales}},
volume = {82},
year = {2011}
}
