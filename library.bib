pages = {1--23},
pmid = {22160726},
publisher = {Annual Reviews},
title = {{Resilience and Stability of Ecological Systems}},
url = {http://arjournals.annualreviews.org/doi/abs/10.1146{\%}2Fannurev.es.04.110173.000245},
volume = {4},
