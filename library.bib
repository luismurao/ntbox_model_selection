journal = {Ecol. Lett.},
keywords = {Lymantria dispar,allee effects,biological invasions,community invasibility,gypsy-moth lepidoptera,invading organisms,invasion speed,lymantriidae,mating,non-indigenous species,plant-communities,propagule pressure,spatial and temporal heterogeneity,stochasticity,success,united-states},
